{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/auth/register": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "methods" : [ "GET, POST, PUT, DELETE, OPTIONS" ],
      "headers" :{
                 "Access-Control-Allow-Origin" :"*"
      }
    }
  ]
}
